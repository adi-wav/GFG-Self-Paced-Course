void func(int  mat[3][2]){}

const int R=3;
const int C=2;
void func(int mat[R][C]){}

//Double pointer
void func(int **arr, int m, int n){}

//Using array of pointers
void func(int *arr[], int m, int n){}

//Using array of vectors
void func(vector<int> a[], int m){}

//Using vector of vectors
void func(vector<vector<int>> &a){}